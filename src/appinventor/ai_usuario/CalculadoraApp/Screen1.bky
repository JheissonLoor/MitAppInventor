<xml xmlns="http://www.w3.org/1999/xhtml">
  <!-- ============================================== -->
  <!-- BLOQUE: Boton Sumar - Click                    -->
  <!-- Suma los dos numeros y muestra el resultado    -->
  <!-- ============================================== -->
  <block type="component_event" id="sumar_click" x="0" y="0">
    <mutation component_type="Button" instance_name="ButtonSumar" event_name="Click"/>
    <field name="COMPONENT_SELECTOR">ButtonSumar</field>
    <statement name="DO">
      <!-- Verificar que los campos no estan vacios -->
      <block type="controls_if" id="sumar_if">
        <mutation elseif="0" else="1"/>
        <value name="IF0">
          <block type="logic_operation" id="sumar_and">
            <field name="OP">AND</field>
            <value name="A">
              <block type="logic_negate" id="sumar_not1">
                <value name="BOOL">
                  <block type="text_isEmpty" id="sumar_empty1">
                    <value name="VALUE">
                      <block type="component_get" id="sumar_get_txt1">
                        <mutation component_type="TextBox" property_name="Text" instance_name="TextBoxNumero1"/>
                        <field name="COMPONENT_SELECTOR">TextBoxNumero1</field>
                        <field name="PROP">Text</field>
                      </block>
                    </value>
                  </block>
                </value>
              </block>
            </value>
            <value name="B">
              <block type="logic_negate" id="sumar_not2">
                <value name="BOOL">
                  <block type="text_isEmpty" id="sumar_empty2">
                    <value name="VALUE">
                      <block type="component_get" id="sumar_get_txt2">
                        <mutation component_type="TextBox" property_name="Text" instance_name="TextBoxNumero2"/>
                        <field name="COMPONENT_SELECTOR">TextBoxNumero2</field>
                        <field name="PROP">Text</field>
                      </block>
                    </value>
                  </block>
                </value>
              </block>
            </value>
          </block>
        </value>
        <statement name="DO0">
          <!-- Establecer LabelOperador a + -->
          <block type="component_set" id="sumar_set_op">
            <mutation component_type="Label" property_name="Text" instance_name="LabelOperador"/>
            <field name="COMPONENT_SELECTOR">LabelOperador</field>
            <field name="PROP">Text</field>
            <value name="VALUE">
              <block type="text" id="sumar_plus_text">
                <field name="TEXT">+</field>
              </block>
            </value>
            <next>
              <!-- Calcular resultado: Numero1 + Numero2 -->
              <block type="component_set" id="sumar_set_result">
                <mutation component_type="Label" property_name="Text" instance_name="LabelResultado"/>
                <field name="COMPONENT_SELECTOR">LabelResultado</field>
                <field name="PROP">Text</field>
                <value name="VALUE">
                  <block type="text_join" id="sumar_join">
                    <mutation items="4"/>
                    <value name="ADD0">
                      <block type="component_get" id="sumar_r_get1">
                        <mutation component_type="TextBox" property_name="Text" instance_name="TextBoxNumero1"/>
                        <field name="COMPONENT_SELECTOR">TextBoxNumero1</field>
                        <field name="PROP">Text</field>
                      </block>
                    </value>
                    <value name="ADD1">
                      <block type="text" id="sumar_r_plus">
                        <field name="TEXT"> + </field>
                      </block>
                    </value>
                    <value name="ADD2">
                      <block type="component_get" id="sumar_r_get2">
                        <mutation component_type="TextBox" property_name="Text" instance_name="TextBoxNumero2"/>
                        <field name="COMPONENT_SELECTOR">TextBoxNumero2</field>
                        <field name="PROP">Text</field>
                      </block>
                    </value>
                    <value name="ADD3">
                      <block type="text_join" id="sumar_r_eq">
                        <mutation items="2"/>
                        <value name="ADD0">
                          <block type="text" id="sumar_r_eq_text">
                            <field name="TEXT"> = </field>
                          </block>
                        </value>
                        <value name="ADD1">
                          <block type="math_arithmetic" id="sumar_calc">
                            <field name="OP">ADD</field>
                            <value name="A">
                              <block type="component_get" id="sumar_c_get1">
                                <mutation component_type="TextBox" property_name="Text" instance_name="TextBoxNumero1"/>
                                <field name="COMPONENT_SELECTOR">TextBoxNumero1</field>
                                <field name="PROP">Text</field>
                              </block>
                            </value>
                            <value name="B">
                              <block type="component_get" id="sumar_c_get2">
                                <mutation component_type="TextBox" property_name="Text" instance_name="TextBoxNumero2"/>
                                <field name="COMPONENT_SELECTOR">TextBoxNumero2</field>
                                <field name="PROP">Text</field>
                              </block>
                            </value>
                          </block>
                        </value>
                      </block>
                    </value>
                  </block>
                </value>
                <next>
                  <!-- Agregar al historial -->
                  <block type="component_set" id="sumar_historial">
                    <mutation component_type="Label" property_name="Text" instance_name="LabelHistorialContenido"/>
                    <field name="COMPONENT_SELECTOR">LabelHistorialContenido</field>
                    <field name="PROP">Text</field>
                    <value name="VALUE">
                      <block type="text_join" id="sumar_hist_join">
                        <mutation items="3"/>
                        <value name="ADD0">
                          <block type="component_get" id="sumar_h_get">
                            <mutation component_type="Label" property_name="Text" instance_name="LabelResultado"/>
                            <field name="COMPONENT_SELECTOR">LabelResultado</field>
                            <field name="PROP">Text</field>
                          </block>
                        </value>
                        <value name="ADD1">
                          <block type="text" id="sumar_h_newline">
                            <field name="TEXT">
</field>
                          </block>
                        </value>
                        <value name="ADD2">
                          <block type="component_get" id="sumar_h_prev">
                            <mutation component_type="Label" property_name="Text" instance_name="LabelHistorialContenido"/>
                            <field name="COMPONENT_SELECTOR">LabelHistorialContenido</field>
                            <field name="PROP">Text</field>
                          </block>
                        </value>
                      </block>
                    </value>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </statement>
        <statement name="ELSE">
          <block type="component_method" id="sumar_notif">
            <mutation component_type="Notifier" method_name="ShowAlert" instance_name="Notifier1"/>
            <field name="COMPONENT_SELECTOR">Notifier1</field>
            <value name="ARG0">
              <block type="text" id="sumar_notif_text">
                <field name="TEXT">Por favor ingresa ambos numeros</field>
              </block>
            </value>
          </block>
        </statement>
      </block>
    </statement>
  </block>

