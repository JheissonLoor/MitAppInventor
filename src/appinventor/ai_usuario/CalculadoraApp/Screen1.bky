  <!-- ============================================== -->
  <!-- BLOQUE: Boton Dividir - Click                  -->
  <!-- Divide los dos numeros con validacion de /0    -->
  <!-- ============================================== -->
  <block type="component_event" id="dividir_click" x="0" y="1050">
    <mutation component_type="Button" instance_name="ButtonDividir" event_name="Click"/>
    <field name="COMPONENT_SELECTOR">ButtonDividir</field>
    <statement name="DO">
      <block type="controls_if" id="div_if_main">
        <mutation elseif="0" else="1"/>
        <value name="IF0">
          <block type="logic_operation" id="div_and">
            <field name="OP">AND</field>
            <value name="A">
              <block type="logic_negate" id="div_not1">
                <value name="BOOL">
                  <block type="text_isEmpty" id="div_empty1">
                    <value name="VALUE">
                      <block type="component_get" id="div_get_txt1">
                        <mutation component_type="TextBox" property_name="Text" instance_name="TextBoxNumero1"/>
                        <field name="COMPONENT_SELECTOR">TextBoxNumero1</field>
                        <field name="PROP">Text</field>
                      </block>
                    </value>
                  </block>
                </value>
              </block>
            </value>
            <value name="B">
              <block type="logic_negate" id="div_not2">
                <value name="BOOL">
                  <block type="text_isEmpty" id="div_empty2">
                    <value name="VALUE">
                      <block type="component_get" id="div_get_txt2">
                        <mutation component_type="TextBox" property_name="Text" instance_name="TextBoxNumero2"/>
                        <field name="COMPONENT_SELECTOR">TextBoxNumero2</field>
                        <field name="PROP">Text</field>
                      </block>
                    </value>
                  </block>
                </value>
              </block>
            </value>
          </block>
        </value>
        <statement name="DO0">
          <!-- Verificar division por cero -->
          <block type="controls_if" id="div_zero_check">
            <mutation elseif="0" else="1"/>
            <value name="IF0">
              <block type="math_compare" id="div_zero_cmp">
                <field name="OP">EQ</field>
                <value name="A">
                  <block type="component_get" id="div_z_get">
                    <mutation component_type="TextBox" property_name="Text" instance_name="TextBoxNumero2"/>
                    <field name="COMPONENT_SELECTOR">TextBoxNumero2</field>
                    <field name="PROP">Text</field>
                  </block>
                </value>
                <value name="B">
                  <block type="math_number" id="div_z_zero">
                    <field name="NUM">0</field>
                  </block>
                </value>
              </block>
            </value>
            <statement name="DO0">
              <!-- Mostrar error: division por cero -->
              <block type="component_method" id="div_zero_notif">
                <mutation component_type="Notifier" method_name="ShowAlert" instance_name="Notifier1"/>
                <field name="COMPONENT_SELECTOR">Notifier1</field>
                <value name="ARG0">
                  <block type="text" id="div_zero_text">
                    <field name="TEXT">Error: No se puede dividir por cero</field>
                  </block>
                </value>
              </block>
            </statement>
            <statement name="ELSE">
              <!-- Realizar la division -->
              <block type="component_set" id="div_set_op">
                <mutation component_type="Label" property_name="Text" instance_name="LabelOperador"/>
                <field name="COMPONENT_SELECTOR">LabelOperador</field>
                <field name="PROP">Text</field>
                <value name="VALUE">
                  <block type="text" id="div_slash_text">
                    <field name="TEXT">/</field>
                  </block>
                </value>
                <next>
                  <block type="component_set" id="div_set_result">
                    <mutation component_type="Label" property_name="Text" instance_name="LabelResultado"/>
                    <field name="COMPONENT_SELECTOR">LabelResultado</field>
                    <field name="PROP">Text</field>
                    <value name="VALUE">
                      <block type="text_join" id="div_join">
                        <mutation items="4"/>
                        <value name="ADD0">
                          <block type="component_get" id="div_r_get1">
                            <mutation component_type="TextBox" property_name="Text" instance_name="TextBoxNumero1"/>
                            <field name="COMPONENT_SELECTOR">TextBoxNumero1</field>
                            <field name="PROP">Text</field>
                          </block>
                        </value>
                        <value name="ADD1">
                          <block type="text" id="div_r_slash">
                            <field name="TEXT"> / </field>
                          </block>
                        </value>
                        <value name="ADD2">
                          <block type="component_get" id="div_r_get2">
                            <mutation component_type="TextBox" property_name="Text" instance_name="TextBoxNumero2"/>
                            <field name="COMPONENT_SELECTOR">TextBoxNumero2</field>
                            <field name="PROP">Text</field>
                          </block>
                        </value>
                        <value name="ADD3">
                          <block type="text_join" id="div_r_eq">
                            <mutation items="2"/>
                            <value name="ADD0">
                              <block type="text" id="div_r_eq_text">
                                <field name="TEXT"> = </field>
                              </block>
                            </value>
                            <value name="ADD1">
                              <block type="math_arithmetic" id="div_calc">
                                <field name="OP">DIVIDE</field>
                                <value name="A">
                                  <block type="component_get" id="div_c_get1">
                                    <mutation component_type="TextBox" property_name="Text" instance_name="TextBoxNumero1"/>
                                    <field name="COMPONENT_SELECTOR">TextBoxNumero1</field>
                                    <field name="PROP">Text</field>
                                  </block>
                                </value>
                                <value name="B">
                                  <block type="component_get" id="div_c_get2">
                                    <mutation component_type="TextBox" property_name="Text" instance_name="TextBoxNumero2"/>
                                    <field name="COMPONENT_SELECTOR">TextBoxNumero2</field>
                                    <field name="PROP">Text</field>
                                  </block>
                                </value>
                              </block>
                            </value>
                          </block>
                        </value>
                      </block>
                    </value>
                    <next>
                      <block type="component_set" id="div_historial">
                        <mutation component_type="Label" property_name="Text" instance_name="LabelHistorialContenido"/>
                        <field name="COMPONENT_SELECTOR">LabelHistorialContenido</field>
                        <field name="PROP">Text</field>
                        <value name="VALUE">
                          <block type="text_join" id="div_hist_join">
                            <mutation items="3"/>
                            <value name="ADD0">
                              <block type="component_get" id="div_h_get">
                                <mutation component_type="Label" property_name="Text" instance_name="LabelResultado"/>
                                <field name="COMPONENT_SELECTOR">LabelResultado</field>
                                <field name="PROP">Text</field>
                              </block>
                            </value>
                            <value name="ADD1">
                              <block type="text" id="div_h_newline">
                                <field name="TEXT">
</field>
                              </block>
                            </value>
                            <value name="ADD2">
                              <block type="component_get" id="div_h_prev">
                                <mutation component_type="Label" property_name="Text" instance_name="LabelHistorialContenido"/>
                                <field name="COMPONENT_SELECTOR">LabelHistorialContenido</field>
                                <field name="PROP">Text</field>
                              </block>
                            </value>
                          </block>
                        </value>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </statement>
          </block>
        </statement>
        <statement name="ELSE">
          <block type="component_method" id="div_notif">
            <mutation component_type="Notifier" method_name="ShowAlert" instance_name="Notifier1"/>
            <field name="COMPONENT_SELECTOR">Notifier1</field>
            <value name="ARG0">
              <block type="text" id="div_notif_text">
                <field name="TEXT">Por favor ingresa ambos numeros</field>
              </block>
            </value>
          </block>
        </statement>
      </block>
    </statement>
  </block>

  <!-- ============================================== -->
  <!-- BLOQUE: Boton Limpiar - Click                  -->
  <!-- Limpia todos los campos y el historial         -->
  <!-- ============================================== -->
  <block type="component_event" id="limpiar_click" x="0" y="1500">
    <mutation component_type="Button" instance_name="ButtonLimpiar" event_name="Click"/>
    <field name="COMPONENT_SELECTOR">ButtonLimpiar</field>
    <statement name="DO">
      <!-- Limpiar TextBox 1 -->
      <block type="component_set" id="limpiar_txt1">
        <mutation component_type="TextBox" property_name="Text" instance_name="TextBoxNumero1"/>
        <field name="COMPONENT_SELECTOR">TextBoxNumero1</field>
        <field name="PROP">Text</field>
        <value name="VALUE">
          <block type="text" id="limpiar_txt1_empty">
            <field name="TEXT"></field>
          </block>
        </value>
        <next>
          <!-- Limpiar TextBox 2 -->
          <block type="component_set" id="limpiar_txt2">
            <mutation component_type="TextBox" property_name="Text" instance_name="TextBoxNumero2"/>
            <field name="COMPONENT_SELECTOR">TextBoxNumero2</field>
            <field name="PROP">Text</field>
            <value name="VALUE">
              <block type="text" id="limpiar_txt2_empty">
                <field name="TEXT"></field>
              </block>
            </value>
            <next>
              <!-- Limpiar Resultado -->
              <block type="component_set" id="limpiar_result">
                <mutation component_type="Label" property_name="Text" instance_name="LabelResultado"/>
                <field name="COMPONENT_SELECTOR">LabelResultado</field>
                <field name="PROP">Text</field>
                <value name="VALUE">
                  <block type="text" id="limpiar_result_zero">
                    <field name="TEXT">0</field>
                  </block>
                </value>
                <next>
                  <!-- Resetear operador -->
                  <block type="component_set" id="limpiar_op">
                    <mutation component_type="Label" property_name="Text" instance_name="LabelOperador"/>
                    <field name="COMPONENT_SELECTOR">LabelOperador</field>
                    <field name="PROP">Text</field>
                    <value name="VALUE">
                      <block type="text" id="limpiar_op_plus">
                        <field name="TEXT">+</field>
                      </block>
                    </value>
                    <next>
                      <!-- Limpiar historial -->
                      <block type="component_set" id="limpiar_hist">
                        <mutation component_type="Label" property_name="Text" instance_name="LabelHistorialContenido"/>
                        <field name="COMPONENT_SELECTOR">LabelHistorialContenido</field>
                        <field name="PROP">Text</field>
                        <value name="VALUE">
                          <block type="text" id="limpiar_hist_empty">
                            <field name="TEXT"></field>
                          </block>
                        </value>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
  </block>
</xml>
